<odoo>
    <data noupdate="1">

        <record id="quality_ncr_report_sequence" model="ir.sequence">
            <field name="name">Quality NCR Report Sequence</field>
            <field name="code">quality_ncr_report_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">NCR/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="quality_ncr_action_sequence" model="ir.sequence">
            <field name="name">NCR Action Sequence</field>
            <field name="code">quality_ncr_action_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">NCR-A/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

         <record id="quality_car_sequence" model="ir.sequence">
            <field name="name">CAR Sequence</field>
            <field name="code">quality_car_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">CAR/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

         <record id="quality_audit_sequence" model="ir.sequence">
            <field name="name">Audit Sequence</field>
            <field name="code">quality_audit_seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">Audit/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
